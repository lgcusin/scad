package ec.uce.edu.biometrico.jpa;

import java.io.Serializable;
import javax.persistence.*;
import java.math.BigDecimal;


/**
 * The persistent class for the CRONOGRAMA_PROCESO_FLUJO database table.
 * 
 */
@Entity
@Table(name="CRONOGRAMA_PROCESO_FLUJO")
@NamedQuery(name="CronogramaProcesoFlujo.findAll", query="SELECT c FROM CronogramaProcesoFlujo c")
public class CronogramaProcesoFlujo implements Serializable {
	private static final long serialVersionUID = 1L;

	@Id
	@Column(name="CRPRFL_ID")
	private long crprflId;

	@Column(name="CRN_ID")
	private BigDecimal crnId;

	@Column(name="CRPRFL_ORDINAL")
	private BigDecimal crprflOrdinal;

	@Column(name="PRFL_ID")
	private BigDecimal prflId;

	public CronogramaProcesoFlujo() {
	}

	public long getCrprflId() {
		return this.crprflId;
	}

	public void setCrprflId(long crprflId) {
		this.crprflId = crprflId;
	}

	public BigDecimal getCrnId() {
		return this.crnId;
	}

	public void setCrnId(BigDecimal crnId) {
		this.crnId = crnId;
	}

	public BigDecimal getCrprflOrdinal() {
		return this.crprflOrdinal;
	}

	public void setCrprflOrdinal(BigDecimal crprflOrdinal) {
		this.crprflOrdinal = crprflOrdinal;
	}

	public BigDecimal getPrflId() {
		return this.prflId;
	}

	public void setPrflId(BigDecimal prflId) {
		this.prflId = prflId;
	}

}