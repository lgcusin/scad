package managedBeans;

import java.util.List;

import javax.annotation.PostConstruct;
import javax.ejb.EJB;

import model.Syllabo;
import servicios.SrvSeguimientoLocal;

public class Syllabus {

	@EJB
	private SrvSeguimientoLocal srvSgm;

	private Syllabo syl;
	public List<Syllabo> syls;

	@PostConstruct
	public void init() {
		
	}

	public void listarSyl(){
		srvSgm.listar(" ");
	}
	
	public Syllabo getSyl() {
		return syl;
	}

	public void setSyl(Syllabo syl) {
		this.syl = syl;
	}

	public List<Syllabo> getSyls() {
		return syls;
	}

	public void setSyls(List<Syllabo> syls) {
		this.syls = syls;
	}
	
	
	

}
